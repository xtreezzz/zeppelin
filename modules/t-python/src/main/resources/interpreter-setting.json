{
  "className": "ru.tinkoff.zeppelin.interpreter.python.PythonInterpreter",
  "properties": {
    "python.working.dir": {
      "envName": "PYTHON_WORKING_DIR",
      "propertyName": "python.working.dir",
      "defaultValue": "",
      "currentValue": "",
      "description": "Path to temp storage of python interpreter results",
      "type": "string"
    },
    "python.jep.library.file": {
      "envName": "PYTHON_JEP_LIBRARY_FILE",
      "propertyName": "python.jep.library.file",
      "defaultValue": "",
      "currentValue": "",
      "description": "Path to JEP *.so library",
      "type": "string"
    },
    "python.jep.config.include.paths": {
      "envName": "PYTHON_JEP_CONFIG_INCLUDE_PATHS",
      "propertyName": "python.jep.config.include.paths",
      "defaultValue": "",
      "currentValue": "",
      "description": "Adds a path of directories separated by File.pathSeparator that will be appended to the sub-intepreter's sys.path",
      "type": "string"
    },
    "python.jep.config.python.home": {
      "envName": "PYTHON_JEP_CONFIG_PYTHON_HOME",
      "propertyName": "python.jep.config.python.home",
      "defaultValue": "",
      "currentValue": "",
      "description": "Set the python's home location, if not specified then use standard",
      "type": "string"
    },
    "python.watchdog.time": {
      "envName": "PYTHON_WATCHDOG_TIME",
      "propertyName": "python.watchdog.time",
      "defaultValue": "-1",
      "currentValue": "-1",
      "description": "Watchdog timeout for external python process (ms)",
      "type": "number"
    },
    "python.subprocess.java.args": {
      "envName": "PYTHON_SUBPROCESS_JAVA_ARGS",
      "propertyName": "python.subprocess.java.args",
      "defaultValue": "",
      "currentValue": "",
      "description": "Additional JVM args(used for inner JVM)",
      "type": "string"
    }
  },
  "editor": {
    "language": "python",
    "editOnDblClick": false,
    "completionSupport": false
  }
}
